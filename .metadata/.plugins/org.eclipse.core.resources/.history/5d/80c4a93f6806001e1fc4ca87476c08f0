// 04
package edu.pnu.service;

import edu.pnu.dao.MemberDao;
import edu.pnu.domain.Member;

public class MemberService {
	
	// 07
	private MemberDao memberDao;
	
	// 09
	public Member getMember(Long id) {
		return memberDao.getMember(id);
		
	}
	
	// 11
	public Member getMember(Long id) {
		return memberDao.getMember(id);
	}

}
